{
  "hosting": {
    "public": "dist",
    "target": "xerris-calendar-app",
    "headers": [
      {
        "source": "/calendar",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://xerris-dashboard-5d23c.web.app"
          }
        ]
      }
    ],
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/xerris-calendar-app.js"
      }
    ]
  }
}
